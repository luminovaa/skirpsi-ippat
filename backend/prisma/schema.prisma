generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model pzem {
  id           Int      @id @default(autoincrement())
  voltage      Float
  current      Float
  power        Float
  frequency    Float
  power_factor Float    @map("pf")
  energy       Float
  va           Float
  var          Float
  created_at   DateTime @default(now())
}

model suhu {
  id          Int   @id @default(autoincrement())
  temperature Float
  created_at   DateTime @default(now())
}


model rpm {
  id Int @id @default(autoincrement())
  rpm Float
  created_at   DateTime @default(now())
}